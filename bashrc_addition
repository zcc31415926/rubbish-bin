export STORAGE=$HOME
export MAIN_ENV=dl
export WORK_DIR=$HOME/Projects

# export PYTHONPATH=$PYTHONPATH:$HOME/miniconda3/envs/dl/lib
export PYTHONPATH=$STORAGE/MyPyModules:$PYTHONPATH
export PYTHONSTARTUP=$HOME/Scripts/startup.py
